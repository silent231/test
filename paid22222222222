local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()




local Window = Rayfield:CreateWindow({
    Name = "Verlox",
    LoadingTitle = "Welcome to Verlox Hub",
    LoadingSubtitle = "Made by Verlox, Modded by RebornPhantom",
    ConfigurationSaving = {
       Enabled = false,
       FolderName = nil, -- Create a custom folder for your hub/game
       FileName = "Verlox Hub Testing"
    },
    Discord = {
       Enabled = false,
       Invite = "noinvitelink", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
       RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
    KeySystem = true, -- Set this to true to use our key system
    KeySettings = {
       Title = "Verlox",
       Subtitle = "Enter key",
       Note = "Warning, this script isn't finshed yet",
       FileName = "resin hub",
       SaveKey = true,
       GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = "verloxon"
    }
 })

-- Load game-specific scripts

if game.PlaceId == 2753915549 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/sasaff94/Reskin-Hub/main/README.md"))()
else if game.PlaceId == 4866692557 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/FallenPhantomRisen/PhantomScripting/main/ReworkedMenu.lua"))()
loadstring(game:HttpGet("https://raw.githubusercontent.com/silent231/private5/main/verloxpaid"))()
else if game.PlaceId == 7952502098 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/silent231/verlox/main/paidverison"))()
else if game.PlaceId == 3527629287 then
loadstring(game:HttpGet("https://raw.githubusercontent.com/radjahfromdiscord/iNEXT/main/source"))()
else 
 loadstring(game:HttpGet("https://raw.githubusercontent.com/silent231/private5/main/verloxpaid"))()
end
end
end
end
